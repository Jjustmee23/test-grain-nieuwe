    "@types/react": "^18.2.42",
    "@types/react-dom": "^18.2.17",
    "@types/node": "^20.10.4",
    "@types/react-beautiful-dnd": "^13.1.8",
    "typescript": "^4.9.5",
    "react-scripts": "5.0.1",
    "@testing-library/react": "^13.4.0",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/user-event": "^13.5.0",
    "web-vitals": "^2.1.4"
