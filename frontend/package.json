{
  "name": "mill-management-frontend",
  "version": "1.0.0",
  "description": "Mill Management Dashboard - React + TypeScript + Material-UI Frontend",
  "private": true,
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
